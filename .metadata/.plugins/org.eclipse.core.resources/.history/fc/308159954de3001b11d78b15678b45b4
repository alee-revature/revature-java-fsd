
public class FunctionalInterfaces {

	public static void main(String[] args) {
		
//		Calc c = new Calc();
//		c.calculate(2)

		Square s = (int input) -> {
			return input * input;
		};
		
		int result = s.calculate(9);
		System.out.println(result);
		
	}
	
}

