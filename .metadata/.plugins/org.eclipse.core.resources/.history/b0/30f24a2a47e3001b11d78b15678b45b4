
public class SerializingObject {
	
	public static void main(String[] args) {
		
		Manager m = new Manager("Mike",34,true);
		
		
		
	}

}

class Manager {
	String name;
	int age;
	boolean senior;
	
	public Manager(String name, int age, boolean senior) {
		this.name = name;
		this.age = age;
		this.senior = senior;
	}
}
